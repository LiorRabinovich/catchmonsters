<?xml version='1.0' encoding='utf-8'?>
<widget id="io.cordova.catchmonsters" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>CatchMonsters</name>
    <description>
        Let's catch monsters
    </description>
    <author email="lior0303@gmail.com">
        Lior Rabinovich
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <preference name="orientation" value="landscape" />
    <preference name="KeepRunning" value="true" />
    <engine name="browser" spec="5.0.1" />
    <engine name="android" spec="6.3.0" />
    <plugin name="cordova-plugin-whitelist" spec="^1.3.3" />
</widget>
